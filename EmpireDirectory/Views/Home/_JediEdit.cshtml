@model EmpireDirectory.Models.Jedi
<input type="text" name="Force" value="@Model.Force" />
<input type="text" name="Lightsaber" value="@Model.LightSaber" />
@{
var x = Model.Rank.ToString();
SelectList list = new SelectList(new List<EmpireDirectory.Models.Rank>()
{
    EmpireDirectory.Models.Rank.Padwan,
    EmpireDirectory.Models.Rank.Jedi,
    EmpireDirectory.Models.Rank.MasterJedi,
    EmpireDirectory.Models.Rank.JediCounsel
}, Model.Rank);
@Html.DropDownList("Rank", list, new { @class="form-control"})
}
